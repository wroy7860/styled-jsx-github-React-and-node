{
  "name": "styled-jsx",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "main": "./dist/flush.js",
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "babel-plugin-syntax-jsx": "^6.18.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.16.0",
    "babel-register": "^6.18.0",
    "fs-promise": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "xo": "^0.17.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react"
    ]
  },
  "scripts": {
    "dev": "gulp",
    "prepublish": "gulp transpile",
    "test": "xo && ava test/styles.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "ignores": [
      "lib/**",
      "test/fixtures/**"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "new-cap": 0
    }
  }
}
